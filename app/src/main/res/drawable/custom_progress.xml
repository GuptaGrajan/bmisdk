<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/_25sdp" />
            <gradient
                android:angle="270"
                android:centerColor="#CACDC9"
                android:centerY="0.75"
                android:endColor="#CACDC9"
                android:startColor="#CACDC9" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/_25sdp" />
                <gradient
                    android:angle="270"
                    android:centerColor="#168974"
                    android:centerY="0.75"
                    android:endColor="#168974"
                    android:startColor="#168974" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/_25sdp" />
                <gradient
                    android:angle="270"
                    android:endColor="#168974"
                    android:startColor="#168974" />
            </shape>
        </clip>
    </item>
</layer-list>
